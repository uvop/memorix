  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:err": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
