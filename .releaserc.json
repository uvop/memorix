{
  "branches": [
    "release",
    {
      "name": "master",
      "prerelease": "rc"
    },
    {
      "name": "next",
      "prerelease": "a"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "../js"
      }
    ],
    [
      "semantic-release-python",
      {
        "setupPy": "./pyproject.toml"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "../../../cli/lib/macos-arm64/memorix",
            "label": "macos-arm64"
          },
          {
            "path": "../../../cli/lib/macos-x64/memorix",
            "label": "macos-x64"
          },
          {
            "path": "../../../cli/lib/linux-x64/memorix",
            "label": "linux-x64"
          },
          {
            "path": "../../../cli/lib/win-x64/memorix.exe",
            "label": "win-x64"
          }
        ]
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "../../../CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["../../../CHANGELOG.md"]
      }
    ]
  ]
}
