{
  "branches": [
    "master",
    {
      "name": "next",
      "prerelease": "rc"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "../js"
      }
    ],
    [
      "semantic-release-python",
      {
        "setupPy": "./pyproject.toml"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "../../../cli/lib/bin/macos-arm64/memorix",
            "label": "macos-arm64"
          },
          {
            "path": "../../../cli/lib/bin/macos-x64/memorix",
            "label": "macos-x64"
          },
          {
            "path": "../../../cli/lib/bin/linux-x64/memorix",
            "label": "linux-x64"
          },
          {
            "path": "../../../cli/lib/bin/win-x64/memorix.exe",
            "label": "win-x64"
          }
        ]
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "../../../CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["../../../CHANGELOG.md"]
      }
    ]
  ]
}
