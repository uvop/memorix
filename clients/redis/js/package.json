{
  "name": "@memorix/client-redis",
  "version": "1.0.0-next.5",
  "description": "",
  "license": "ISC",
  "author": "",
  "main": "lib/src/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "codegen": "yarn codegen-example",
    "codegen-example": "yarn codegen-example-1 && yarn codegen-example-replace-1",
    "codegen-example-1": "(cd ../../../cli && yarn cli codegen ../clients/redis/js/src/example-schema.memorix typescript ../clients/redis/js/src)",
    "codegen-example-replace-1": "sed -i '' 's|@memorix/client-redis|./index|g' ./src/example-schema.generated.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:err": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "start": "ts-node -r tsconfig-paths/register src/start.ts",
    "test": "jest",
    "test:dev": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "callback-to-async-iterator": "^1.1.1",
    "ioredis": "^5.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/callback-to-async-iterator": "^1.1.4",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.28",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "28.0.0",
    "prettier": "^2.1.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
