{
  "name": "@memorix/client-js",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "main": "lib/src/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:err": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "codegen-example": "cd ../../codegen && yarn generate-schema ../clients/js/src/example-schema.memorix",
    "codegen-report": "yarn codegen-report-1 && yarn codegen-report-replace-1 && yarn codegen-report-replace-2",
    "codegen-report-1": "(cd ../../codegen && yarn generate-schema ../report-schema/schema.memorix ../clients/js/src/report-api.generated.ts)",
    "codegen-report-replace-1": "sed -i '' 's|MemorixApi|ReportApi|g' ./src/report-api.generated.ts",
    "codegen-report-replace-2": "sed -i '' 's|@memorix/client-js|./BaseReportApi|g' ./src/report-api.generated.ts"
  },
  "devDependencies": {
    "@types/node": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
